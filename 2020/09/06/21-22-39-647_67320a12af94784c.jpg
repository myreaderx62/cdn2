		$(document).ready(function(){fetch_lsv();});
		function delay() 
		{
			return 1000 + Math.floor (Math.random()* 100);
		}
		function retry_connect() 
		{
			fetch_lsv ();
			goleft();
		}
		function display_lsv () 
		{
			if (cur >= lsvs.length )
				cur = lsvs.length - 1;

			$("#lsvaccueil").html ("<h1>"+lsvs[cur][0]+"</h1>"+lsvs[cur][1]+"<a id=\"lire-suite\"href=\""+lsvs[cur][2]+"\"><img src=\"https://static.spontex.org/css/style/imgs/apprendre-davantage-en.png\" alt=\"Know more\" /></a>");
		}
		function fetch_lsv ()
		{
			if(still_fetching)
				return;
			if ( ((lsvs.length - cur) < 10) && ( lsvs[lsvs.length-1][3] >= 0) )
				do_fetch_lsv ();
		}
		function do_fetch_lsv ()
		{
			still_fetching = true;
			$("#loading").fadeIn("slow");
			var request = $.ajax({
                                url: "/android/random/",
                                dataType: "json"});
                        request.done ( function(msg) {
				$("#loading").fadeOut("slow");
				var added = 0;
				for (var i=0;i<msg.length;i++) {
					var double = false;
					for (var j = 0;j<lsvs.length;j++) {
						if ( lsvs[j][3] == msg[i].id ) {
							double = true; break;
						}
					}
					if ( ! double ) {
						added++; lsvs.push ( [msg[i].titre, msg[i].resume, msg[i].url.replace("http:", ""), msg[i].id] );
					}
				}
				if (added == 0 && (lsvs[lsvs.length - 1][3] != -1)) {
					lsvs.push (["More DYKs!", "The loading of 10 DYK have made NO new. It's possible you've already loaded a bunch of them, or you're pretty unlucky... Or you voted for (almost) every DYKs : it's advised to vote for the few you didn't, if that's the case.", "/le_saviez_vous/random/", -1]);
				}
				if (cur < ( lsvs.length - 1) )
					$("#lsvaccueil-fleche-d").fadeIn("slow");
				still_fetching = false;
                       	});
                        request.fail ( function(x, e) {
				if ( lsvs[lsvs.length - 1][3] == 0 ) {
					lsvs.pop();
				}
				still_fetching = false;
				$("#loading").fadeOut("slow");
				lsvs.push(["CONNEXION TROUBLE", "The connexion to the server has failed :please <a href='Javascript:retry_connect();'>retry</a>!", "/le_saviez_vous/548/totally-useless-dyk", 0]);
				if (cur < ( lsvs.length - 1) )
					$("#lsvaccueil-fleche-d").fadeIn("slow");
                        });
		}
		function goleft()
		{
			if (en_cours)
				return;
			if (cur <= 0) {
				return;
			}
			en_cours = true;
			$("#lsvaccueil-fleche-g").fadeOut("slow");
